}

void mousePressed(){
  if(Pantalla == 0){
    Pantalla = 1;
  }
}

void keyPressed(){
  if(Pantalla == 1){
   if(key == 'u' || key == 'U'){
     
     PokemonToma = 1;
     Pantalla = 2;
     
   }else if(key == 'h' || key == 'H'){
   PokemonToma = 2;
     Pantalla = 2;
   }else if(key == 'b' || key == 'B'){
   PokemonToma = 3;
     Pantalla = 2;
   }else if(key == 'y' || key == 'Y'){
   PokemonToma = 4;
     Pantalla = 2;
   }else if(key == 'g' || key == 'G'){
   PokemonToma = 5;
     Pantalla = 2;
   }else if(key == 'v' || key == 'V'){
   PokemonToma = 6;
     Pantalla = 2;
   }
  }else if(Pantalla == 2){
    if(key == 'a' || key == 'A'){
      Pantalla = 3;
   }
  }else if(Pantalla == 3){
    if(key == 'z' || key == 'Z'){
      Pantalla = 0;
   }
  }
}
